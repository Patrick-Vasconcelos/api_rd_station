pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: 'main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Patrick-Vasconcelos/api_rd_station.git']]])
            }
        }
        stage('Build') {
            steps {
                git branch: 'main', url: 'https://github.com/Patrick-Vasconcelos/api_rd_station.git'
                sh 'python3 test.py'
            }
        }
    
    }
}